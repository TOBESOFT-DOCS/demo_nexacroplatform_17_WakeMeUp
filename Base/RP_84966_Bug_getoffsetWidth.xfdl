                  <Format id="default">
                    <Columns>
                      <Column size="26"/>
                      <Column size="237"/>
                    </Columns>
                    <Rows>
                      <Row size="24"/>
                    </Rows>
                    <Band id="body">
                      <Cell text="bind:CHK" displaytype="checkboxcontrol" edittype="checkbox"/>
                      <Cell col="1" text="bind:STR"/>
                    </Band>
                  </Format>
                </Formats>
              </Grid>
              <Button id="btn_ok" taborder="2" text="OK" left="110" width="51" bottom="5" height="24" onclick="div_filterByValue_btn_ok_onclick"/>
              <Button id="btn_chg2" taborder="3" text="Custom Filter" top="0" width="93" height="24" right="0" onclick="btn_chg_onclick"/>
            </Layout>
          </Layouts>
        </Div>
        <Button id="btn_chg" taborder="5" top="5" width="16" height="15" right="btn_close:5" onclick="btn_chg_onclick" tooltiptext="btn_chg" cssclass="btn_WF_InW"/>
        <Button id="Button01" taborder="6" text="Button00" left="61" top="6" width="70" height="22" onclick="Button01_onclick"/>
      </Layout>
    </Layouts>
    <Objects>
    </Objects>
    <Script type="xscript5.1"><![CDATA[
 
this.form_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.fv_Keystring="";
	this.fv_Filterstring="";
