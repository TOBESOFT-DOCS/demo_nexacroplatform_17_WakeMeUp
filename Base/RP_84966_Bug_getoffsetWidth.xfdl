<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.0">
  <Form id="grd_head_popup" width="270" height="270" titletext="Favorite Configuration" onload="form_onload" background="white" scrollbartype="none none" scrolltype="none">
    <Layouts>
      <Layout height="270" mobileorientation="landscape" width="270">
        <Static id="sta_title" text="Col Name" wordWrap="none" onclick="Static00_onclick" left="5" top="5" height="24" taborder="0" right="26" cssclass="sta_WF_TitleA" fittocontents="height"/>
        <Button id="btn_close" taborder="1" onclick="div_sort_btn_ok_onclick" top="5" width="16" height="15" right="5" tooltiptext="btn_close" cssclass="btn_WF_Close"/>
        <Div id="div_sort" taborder="2" text="div_00" left="0" top="sta_title:5" bottom="0" background="white" right="0">
          <Layouts>
            <Layout>
              <Button id="btn_del" taborder="1" text="Del col" top="0" width="63" height="24" right="0" onclick="div_sort_btn_del_onclick"/>
              <Button id="btn_add" taborder="0" text="Add col" top="0" height="24" right="btn_del:5" width="63" onclick="div_sort_btn_add_onclick"/>
              <Grid id="grd_sort" taborder="2" left="0" top="btn_add:5" right="0" bottom="34" binddataset="DS_SORT" autofittype="col" onheadclick="div_sort_grd_sort_onheadclick" autoenter="select" cssclass="grd_WF_PoptxtLine">
                <Formats>
                  <Format id="default">
                    <Columns>
                      <Column size="24"/>
                      <Column size="120"/>
                      <Column size="120"/>
                    </Columns>
                    <Rows>
                      <Row size="24" band="head"/>
                      <Row size="24"/>
                    </Rows>
                    <Band id="head">
                      <Cell displaytype="checkboxcontrol" edittype="checkbox" text="0" autosizecol="none"/>
                      <Cell col="1" text="COL"/>
                      <Cell col="2" text="TYPE"/>
                    </Band>
                    <Band id="body">
                      <Cell displaytype="checkboxcontrol" edittype="checkbox" text="bind:__CHK" autosizecol="none"/>
                      <Cell col="1" text="bind:COL" displaytype="combocontrol" edittype="combo" combodataset="DS_COL" combodatacol="HEAD_TEXT" combocodecol="BODY_BIND"/>
                      <Cell col="2" text="bind:TYPE" displaytype="combocontrol" edittype="combo" combodataset="DS_SORT_TYPE" combodatacol="TYPE" combocodecol="ID"/>
                    </Band>
                  </Format>
                </Formats>
              </Grid>
              <Button id="btn_ok" taborder="3" text="OK" left="110" width="51" height="24" bottom="5" onclick="div_sort_btn_ok_onclick"/>
            </Layout>
          </Layouts>
        </Div>
        <Div id="div_filter" taborder="3" text="div_00" left="283" top="sta_title:5" width="270" bottom="0" background="white">
