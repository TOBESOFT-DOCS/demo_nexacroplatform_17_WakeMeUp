            <Layout>
              <Static id="sta_titleF" taborder="0" text="Custom Filter" left="0" top="0" height="24" right="0"/>
              <Combo id="cbo_cond" taborder="1" text="cbo_00" left="5" top="50" width="112" height="24" innerdataset="DS_FILTER_COND" codecolumn="CD" datacolumn="NM"/>
              <Combo id="cbo_val" taborder="2" text="cbo_00" left="123" top="50" height="24" innerdataset="DS_FILTER" codecolumn="STR" datacolumn="STR" onitemchanged="div_filter_cbo_val_onitemchanged" right="5"/>
              <Radio id="rdo_type" taborder="3" left="77" top="77" height="24" innerdataset="DS_FILTER_ADD" codecolumn="CD" datacolumn="NM" direction="vertical" right="62" index="0" text="And" value="1" columncount="2"/>
              <Combo id="cbo_cond00" taborder="4" text="cbo_00" left="5" top="110" width="112" height="24" innerdataset="DS_FILTER_COND" codecolumn="CD" datacolumn="NM"/>
              <Combo id="cbo_val00" taborder="5" text="cbo_00" left="123" top="110" height="24" innerdataset="DS_FILTER" codecolumn="STR" datacolumn="STR" onitemchanged="div_filter_cbo_val00_onitemchanged" right="5"/>
              <Button id="btn_ok" taborder="6" text="OK" left="86" width="51" bottom="5" height="24" onclick="div_filter_btn_ok_onclick"/>
              <Button id="btn_cancle" taborder="7" text="Cancel" left="btn_ok:5" width="61" bottom="5" height="24" onclick="div_filter_btn_cancle_onclick"/>
              <Edit id="edt_val" taborder="8" left="123" top="50" height="24" right="29"/>
              <Edit id="edt_val00" taborder="9" left="123" top="110" height="24" right="29"/>
            </Layout>
          </Layouts>
        </Div>
        <Div id="div_filterByValue" taborder="4" text="div_00" left="620" top="sta_title:5" width="270" background="white" bottom="0">
